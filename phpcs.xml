<?xml version="1.0"?>
<ruleset name="Custom">
    <rule ref="PSR12">
        <exclude name="Generic.Files.LineEndings"/>
    </rule>
     <rule ref="Generic.Files.LineLength">
        <properties>
            <property name="lineLimit" value="150"/>
        </properties>
    </rule>
    <exclude-pattern>vendor/</exclude-pattern>
</ruleset>
